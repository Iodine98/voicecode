.clickable-command
	font-size 10px
	line-height 14px
	cursor pointer
	display inline-block
	background white
	padding 0 2px
	font-family Verdana
	&.current
		color red
		
.commandTag
	font-size 12px
	line-height 14px
	cursor pointer
	display inline-block
	background white
	padding 1px 3px
	margin 1px 3px
	font-family Verdana
	border 1px solid #ddd
	&.current
		color red