{
  "restartable": "restart",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": false,
  "execMap": {
    "coffee": "coffee --compile --map --output dist"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "src",
    "package.json"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "coffee json"
}
